# Firebase Functions local development variables
# Copy to `.env` when emulating functions locally.

# Upstream property API endpoint for scheduled commission sync
PROPERTY_API_URL=__REPLACE_ME_PROPERTY_API_URL__

# Internal tokens used to authenticate outbound requests
INTERNAL_SERVICE_TOKEN=__REPLACE_ME_INTERNAL_SERVICE_TOKEN__
PROPERTY_INTERNAL_TOKEN=__REPLACE_ME_PROPERTY_INTERNAL_TOKEN__
AI_INTERNAL_TOKEN=__REPLACE_ME_AI_INTERNAL_TOKEN__

# ==========================================================
# ðŸ¤– AI CHAT & PROXY SETTINGS
# ==========================================================
AI_SERVICE_URL=__REPLACE_ME_AI_SERVICE_URL__
AI_PROXY_STREAMING_MODE=chunked
ALLOW_ANONYMOUS_AI_CHAT=false
ENABLE_METADATA=true
ENABLE_AI_ROLLOUT_PROXY=false
AI_ROLLOUT=blue
AI_ROLLOUT_PERCENTAGE=100
AI_ROLLOUT_USER_GROUPS=

# Primary & fallback model providers
GROQ_API_KEY=__REPLACE_ME_GROQ_API_KEY__
OPENAI_API_KEY=__REPLACE_ME_OPENAI_API_KEY__
