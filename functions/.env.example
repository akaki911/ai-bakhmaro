# Firebase Functions local development variables
# Copy to `.env` when emulating functions locally.

# Upstream property API endpoint for scheduled commission sync
PROPERTY_API_URL=https://api.example.local/property

# Internal tokens used to authenticate outbound requests
INTERNAL_SERVICE_TOKEN=example_internal_service_token_c1d2e3f40516273849ab5c6d7e8f90a1
PROPERTY_INTERNAL_TOKEN=example_property_internal_token_abc123def456
AI_INTERNAL_TOKEN=example_ai_internal_token_b1c2d3e4f5a60718293b4c5d6e7f8091

# ==========================================================
# ðŸ¤– AI CHAT & PROXY SETTINGS
# ==========================================================
AI_SERVICE_URL=https://api.example.local/api/ai
AI_PROXY_STREAMING_MODE=chunked
ALLOW_ANONYMOUS_AI_CHAT=false
ENABLE_METADATA=true
ENABLE_AI_ROLLOUT_PROXY=false
AI_ROLLOUT=blue
AI_ROLLOUT_PERCENTAGE=100
